{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "apis/*": [
                "./src/apis/*"
            ],
            "utils/*": [
                "./src/utils/*"
            ],
            "pages/*": [
                "./src/pages/*"
            ]
        },
        "moduleResolution": "node",
        "target": "esnext"
    }
}